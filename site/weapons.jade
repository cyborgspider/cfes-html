    script.
      $(function(){
          function filterWeapons(weaponType){
            $('.weapon-list-item:not([data-type="'+weaponType+'"])').hide();
            $('.weapon-list-item[data-type='+weaponType+']').show();
          }

          $('.weapon-list-item').on('click','.weapon-header',
            function(){
              $(this).toggleClass('active');
              $(this).next('.weapon-info').slideToggle(100);
            }
          )
          $('.weapon-nav').on('click','.btn',
            function(e){
              e.preventDefault();
              var weaponType= $(this).attr('href').substring(1);
              $(this).addClass('active');
              $(this).siblings().removeClass('active');
              filterWeapons(weaponType);
            }
          )

          filterWeapons('assalto');
        });
