    script(src="http://code.jquery.com/jquery-2.1.0.min.js", type="text/javascript")
    script.
      $(function(){
          $('.weapon-list-item').on('click','.weapon-header',
            function(){
              $(this).toggleClass('active');
              $(this).next('.weapon-info').slideToggle(100);
            }
          )
        });
  body
    //-Middle content
    #newContent
      nav.weapon-nav
        a.btn.btn-long(href="#") Fuzis de Assalto
        a.btn.btn-long(href="#") Submetralhadoras
        a.btn.btn-long(href="#") Fuzis de Precisão
        a.btn.btn-long(href="#") Metralhadoras
        a.btn.btn-long(href="#") Escopetas
        a.btn.btn-long(href="#") Pistolas
        a.btn.btn-long(href="#") Equipamento
        a.btn.btn-long(href="#") Arremesso
        a.btn.btn-long(href="#") Armas Brancas

      ul.weapon-list
        li.weapon-list-item
          .weapon-header M16
          .weapon-info
            p.label Descrição
            p Fuzil padrão das tropas americanas.
            p.label Especificação
            dl.spec-list
              dt Fabricação:
              dd Colt Defense
              dt Origem:
              dd Estados Unidos
              dt Cartucho:
              dd 5.56x45mm NATO
              dt Modos de Tiro:
              dd Automática
              dt Disponibilidade:
              dd Padrão
            p.label Preço
            p Padrão
          .weapon-view
            img.weapon-img(src="img/weapons/weapon-AK-47_Knife.png", alt="Weapon AK")
            .weapon-text MUNIÇÃO : <span class="text-highlight">30 / 90</span>
            ul.stat-list
              li.stat-list-item
                p Potência
                .stat-bar
                  .stat-bar-fill(style="width:20%")
              li.stat-list-item
                p Precisão
                .stat-bar
                  .stat-bar-fill.special(style="width:70%")
              li.stat-list-item
                p Velocidade
                .stat-bar
                  .stat-bar-fill(style="width:30%")
              li.stat-list-item
                p Recuo
                .stat-bar
                  .stat-bar-fill(style="width:15%")
              li.stat-list-item
                p Peso
                .stat-bar
                  .stat-bar-fill(style="width:99%")




