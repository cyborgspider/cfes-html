    script.
      $(function(){

          function switchTab(tabContent){
            console.log(tabContent);
            $('.tab-content:not([data-tab-content="'+tabContent+'"])').hide();
            $('.tab-content[data-tab-content="'+tabContent+'"]').show();
          }

          $('.tabs').on('click','.tab-link',
            function(e){
              e.preventDefault();
              var tabContent = $(this).attr('href').substring(1);
              $(this).parent('.tab').addClass('active');
              $(this).parent('.tab').siblings().removeClass('active');
              switchTab(tabContent);
            }
          );

        });
